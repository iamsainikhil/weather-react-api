{
  "version": 2,
  "builds": [{"src": "index.js", "use": "@now/node-server"}],
  "routes": [
    {
      "src": ".*",
      "methods": ["GET", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": [
          "https://iamsainikhil.github.io",
          "http://localhost:3000"
        ]
      },
      "dest": "/index.js",
      "continue": true
    },
    {
      "src": "/forecast/coords/(?<latlong>[^/]+)",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/address/coords/(?<latlong>[^/]+)",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/places/query/(?<city>[^/]+)/(?<latlong>[^/]+)",
      "dest": "/index.js",
      "methods": ["GET"]
    }
  ]
}
