{
  "version": 2,
  "builds": [{"src": "index.js", "use": "@now/node-server"}],
  "routes": [
    {
      "src": ".*",
      "methods": ["GET", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "http://localhost:3000"
      },
      "continue": true
    },
    {
      "src": "/forecast/coords/(?<latlong>[^/]+)",
      "dest": "/index.js"
    },
    {
      "src": "/address/coords/(?<latlong>[^/]+)",
      "dest": "/index.js"
    },
    {
      "src": "/places/query/(?<city>[^/]+)/",
      "dest": "https://weather-react-api-dev.now.sh/places/query/(?<city>[^/]+)/00,00"
    },
    {
      "src": "/places/query/(?<city>[^/]+)/(?<latlong>[^/]+)",
      "dest": "/index.js"
    }
  ]
}
