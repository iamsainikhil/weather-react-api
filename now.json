{
  "version": 2,
  "builds": [{"src": "index.js", "use": "@now/node-server"}],
  "routes": [
    {
      "src": "/forecast/coords/(?<latlong>[^/]+)",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/address/coords/(?<city>[^/]+)/(?<latlong>[^/]+)",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/places/query/(?<city>[^/]+)/(?<latlong>[^/]+)",
      "dest": "/index.js",
      "methods": ["GET"]
    }
  ]
}
