{
  "version": 2,
  "builds": [{"src": "index.js", "use": "@now/node-server"}],
  "routes": [
    {
      "src": "/forecast/coords/(?<latlong>[^/]+)",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/address/coords/(?<latlong>[^/]+)",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/places/query/(?<city>[^/]+)/(?<latlong>[^/]+)",
      "dest": "/index.js",
      "methods": ["GET"]
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://iamsainikhil.github.io/weather-react"
        }
      ]
    }
  ]
}
